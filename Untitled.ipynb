   "source": [
    "import numpy as np\n",
    "import numpy.linalg as la\n",
    "# **************************** \n",
    "Coeffs=[2,3,1]\n",
    "n=len(Coeffs)-1 # degré de Q \n",
    "A=np.eye(n+1,n+1)\n",
    "Diag=[k for k in range(1,n+1)] \n",
    "A=A+np.diag(Diag,1)\n",
    "Sol = la.solve(A,Coeffs) # résolution du système linéaire A*P=Q\n",
    "print(Sol)"
   ]
